{
  "name": "jsdoc",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_begin"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "block_tag"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_end"
        }
      ]
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_text"
              },
              {
                "type": "SYMBOL",
                "name": "inline_tag"
              }
            ]
          }
        }
      ]
    },
    "block_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "{"
                },
                {
                  "type": "SYMBOL",
                  "name": "type"
                },
                {
                  "type": "STRING",
                  "value": "}"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "description"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "inline_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "@[a-z_]+"
    },
    "type": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "_text": {
      "type": "PATTERN",
      "value": "[^\\*@\\s{}][^{}\\n]*"
    },
    "_begin": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "STRING",
              "value": "*"
            }
          }
        ]
      }
    },
    "_end": {
      "type": "STRING",
      "value": "/"
    }
  },
  "extras": [
    {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\n"
              },
              {
                "type": "PATTERN",
                "value": "[ \\t]*"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "*"
                    },
                    {
                      "type": "PATTERN",
                      "value": "[ \\t]*"
                    }
                  ]
                }
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[ \\t]"
          }
        ]
      }
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": []
}